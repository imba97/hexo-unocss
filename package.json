{
  "name": "hexo-unocss",
  "type": "module",
  "version": "1.0.5",
  "packageManager": "pnpm@10.13.0",
  "description": "Support using unocsss when writing articles",
  "author": "imba97",
  "license": "MIT",
  "homepage": "https://github.com/imba97/hexo-unocss",
  "repository": {
    "type": "git",
    "url": "https://github.com/imba97/hexo-unocss"
  },
  "bugs": {
    "url": "https://github.com/imba97/hexo-unocss/issues"
  },
  "keywords": [
    "hexo",
    "plugin",
    "unocss"
  ],
  "main": "./dist/index.cjs",
  "files": [
    "./dist/index.cjs"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "stub": "cross-env NODE_ENV=development unbuild --stub",
    "build": "cross-env NODE_ENV=production unbuild",
    "lint": "eslint --cache",
    "release": "bumpp"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "@unocss/cli": "latest"
  },
  "optionalDependencies": {
    "@iconify/json": "latest"
  },
  "devDependencies": {
    "@imba97/eslint-config": "^0.0.6",
    "@types/node": "^20.19.6",
    "bumpp": "^10.2.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "hexo": "^7.3.0",
    "lint-staged": "^16.1.2",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --cache --fix"
  }
}
